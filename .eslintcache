h, useSelector } from 'react-redux';\nimport { handleProfileActionCreator } from './store/actions/profile';\nimport { GET_PROFILE_FAILED } from './store/actionTypes';\n\nimport 'antd/dist/antd.css';\nimport './assets/styles/styles.scss';\n\nimport { MainPage, DashboardPage, ActivatePage } from './routes/routes';\nimport PrivateRoute from './routes/PrivateRoute';\n\n\nconst App = () => {\n  const {isLog} = useSelector( state => ({\n    isLog: state.profile.isLog\n  }))\n  const dispatch = useDispatch()\n\n  useEffect(() => {\n    const token = localStorage.getItem('token')\n    if(token) {\n      dispatch(handleProfileActionCreator())\n    }\n    else dispatch({ type: GET_PROFILE_FAILED })\n  }, [isLog])\n\n  return (\n    <BrowserRouter>\n      <Switch>\n        <Route path='/' component={MainPage} exact/>\n        <PrivateRoute exact path=\"/dashboard\">\n          <DashboardPage />\n        </PrivateRoute>\n        <Route path=\"/activation/:uid/:token\" component={ActivatePage} exact/>\n      </Switch>\n    </BrowserRouter>\n  )\n}\n\nexport default App\n\n\n","/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/index.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/actionTypes.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/actions/profile.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/routes/routes.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/routes/PrivateRoute.js",["202"],"import React from 'react';\nimport { Route, Redirect } from 'react-router-dom';\nimport { useSelector } from 'react-redux';\n\nconst PrivateRoute = ({children,...rest}) => {\n  const {isLog, success, failed } = useSelector(state => ({\n    isLog: state.profile.isLog,\n    // loading: state.profile.get.loading,\n    success: state.profile.get.success,\n    failed: state.profile.get.failed,\n  }))\n\n    return (<Route {...rest} render={()=> (\n      isLog ? (children) : failed ? (<Redirect to={'/'}/>) : null\n      )}/>);\n}\n\nexport default PrivateRoute;","/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/profile.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/works.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/signUp.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/signIn.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/themeSwitcher.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/reducers/activate.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/axios/axios.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/pages/MainPage.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/pages/ActivatePage.js",["203"],"import React, {useEffect} from 'react';\nimport { useParams, useHistory, NavLink } from 'react-router-dom';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { handleActivateActionCreator } from '../store/actions/activate';\n\nimport { Footer } from '../components/';\n\nimport Logo from '../assets/images/logo.png';\n\nconst ActivatePage = () => {\n  const {loading, success, failed, theme} = useSelector(state => ({\n    success: state.activate.post.success,\n    loading: state.activate.post.loading,\n    failed: state.activate.post.failed,\n    theme: state.themeSwitcher.theme\n  }))\n\n  const { uid, token } = useParams()\n  const dispatch = useDispatch()\n  let history = useHistory();\n\n  const handleRedirect = () => {\n    history.push('');\n  }\n  useEffect(() => {\n   dispatch(handleActivateActionCreator(uid, token, handleRedirect))\n  }, [])\n  \n\n  return (\n    <div className={`page ${theme === 'dark' && 'theme-dark'}`}>\n      <header className='activation-header'>\n        <div className='container'>\n          <NavLink to='/' exact><img src={Logo} className='logo' alt='logo' /></NavLink>\n        </div>\n      </header>\n      <section className='activation-content container'>\n        {\n          loading ? 'loading' :\n          success ? \n          <>\n            <h1 className='title'>Welcome to <span>DropMakers</span>!</h1>\n            <p className='text'>Thank you, your account has been successfully created</p>\n          </> :\n          failed ? <p className='text'>Something went wrong, please try refreshing the page</p> : null\n        }\n      </section>\n      <Footer />\n    </div>\n  )\n}\n\nexport default ActivatePage\n\n\n\n\n","/Users/user/Desktop/myprojects/DropMakers/src/pages/DashboardPage.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/actions/activate.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/index.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Template/PrivatePageTemplate.js",["204"],"import React, {useState}  from 'react';\nimport { useSelector } from 'react-redux';\n\nimport { PrivateHeader, Footer } from '../index';\n\nconst PrivatePageTemplate = (props) => {\n  const {theme} = useSelector((state) => ({\n    theme: state.themeSwitcher.theme\n  }))\n\n  return (\n    <div className={`page ${theme === 'dark' && 'theme-dark'}`}>\n      <PrivateHeader />\n      <main style={{minHeight: '75vh'}}>\n        {props.children}\n      </main>\n      <Footer />\n    </div>\n  )\n}\n\nexport default PrivatePageTemplate\n","/Users/user/Desktop/myprojects/DropMakers/src/components/Template/PageTemplate.js",["205"],"import React, {useState} from 'react';\nimport { useSelector } from 'react-redux';\n// import { BackTop } from 'antd';\n\nimport { Header, Footer } from '../index';\n\n// Обертка для всего сайта\nconst PageTemplate = (props) => {\n  const {theme} = useSelector((state) => ({\n    theme: state.themeSwitcher.theme\n  }))\n\n  return (\n    <div className={`page ${theme === 'dark' && 'theme-dark'}`}>\n      <Header />\n      <main>\n        {props.children}\n        {/* <BackTop>\n          <div className='back-top'>Top</div>\n        </BackTop> */}\n      </main>\n      <Footer />\n    </div>\n  )\n}\n\nexport default PageTemplate","/Users/user/Desktop/myprojects/DropMakers/src/components/Switcher/ThemeSwitcher.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Layout/PrivateHeader.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Layout/Footer.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Layout/Header.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/SVG/search.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/SVG/send.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/SVG/settings.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/MainPage/Steps.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/SVG/logo.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/MainPage/Description.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/MainPage/Login.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/MainPage/Requirements.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/MainPage/Features.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/MainPage/About.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Forms/SignForm/SignForm.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Forms/SignForm/GoogleSign.js",["206","207"],"import React, {useEffect} from 'react';\nimport { useDispatch } from 'react-redux';\nimport {handleGoogleSignOutActionCreator, \n  handleGoogleActionCreator, handleGoogleSignInActionCreator} from '../../../store/actions/googleAuth'\n\nimport GoogleIcon from '../../../assets/images/google.png';\n\nconst GoogleSign = () => {\n\n  const dispatch = useDispatch()\n\n  useEffect(() => {\n    dispatch(handleGoogleActionCreator())\n  }, [])\n\n  const handleGoogleIn = () => {\n    dispatch(handleGoogleSignInActionCreator())\n  }\n\n  const handleGoogleOut = () => {\n    dispatch(handleGoogleSignOutActionCreator)\n  }\n\n  return (\n    <button className='google' onClick={handleGoogleIn}>\n      <img src={GoogleIcon} alt='google' className='google-icon'/>\n      <p className='google-text'>Sign up with Google</p>\n    </button>\n  )\n}\n\nexport default GoogleSign\n","/Users/user/Desktop/myprojects/DropMakers/src/components/Forms/ContactsForm.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Forms/SignForm/SignIn.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Forms/SignForm/SignUp.js",["208"],"import React, {useState} from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { handleSignUpActionCreator } from '../../../store/actions/signUp';\nimport { GoogleSign } from '../../';\n\nimport {Formik, Form, Field, ErrorMessage} from 'formik';\nimport * as Yup from 'yup';\n\nimport LogoIcon from '../../../assets/images/logo.png';\n\nconst SignUp = ({setSignUp}) => {\n  const {loading, success, failed} = useSelector(state => ({\n    success: state.signUp.post.success,\n    loading: state.signUp.post.loading,\n    failed: state.signUp.post.failed\n  }))\n  \n  const dispatch = useDispatch()\n\n  return (\n    <section className='signUp'>\n      <div className='signUp-left'>\n        <img src={LogoIcon} alt='logo' className='logo'/>\n        { success ? <p className='success-text'>A letter has been sent to your mail. Please confirm your mail</p> :\n        <>\n        <div className='title-content'>\n          <h1 className='title'>Create an account</h1>\n          <div>* all fields must be filled</div>\n        </div>\n        <div className='title-descr'>User registration</div>\n        <GoogleSign />\n        <div className='line'>\n          <div className='line-1'></div>\n          <div className='line-text'>OR</div>\n          <div className='line-2'></div>\n        </div>\n        <Formik\n          initialValues={\n            {\n              first_name: '',\n              last_name: '',\n              email: '',\n              password: '',\n              confirmPassword: '',\n              acceptTerms: false\n            }\n          }\n          validationSchema={\n            Yup.object().shape({\n              first_name: Yup.string()\n                .required('Required'),\n              last_name: Yup.string()\n                .required('Required'),\n              email: Yup.string()\n                .required('Required')\n                .email('Invalid email').required('Required'),\n              password: Yup.string()\n                .min(6, 'Minimum 6 characters')\n                .required('Required'),\n              confirmPassword: Yup.string()\n                .oneOf([Yup.ref('password'), null], 'Confirm password')\n                .required('Required'),\n              acceptTerms: Yup.bool().oneOf([true], 'Check the box')\n            })\n          }\n          onSubmit ={\n            fields => {\n             dispatch(handleSignUpActionCreator(fields))\n            }\n          } >\n          {() => (\n            <Form className='form'>\n              <div className='fullname'>\n                <div>\n                  <div className='label'>* First Name</div>\n                  <Field type=\"text\" name=\"first_name\" />\n                  <ErrorMessage name=\"first_name\" component=\"div\" className='data-error'/>\n                </div>\n                <div>\n                  <div className='label'>* Last Name</div>\n                  <Field type=\"text\" name=\"last_name\" />\n                  <ErrorMessage name=\"last_name\" component=\"div\" className='data-error'/>\n                </div>\n              </div>\n              <div className='label'>* Email</div>\n              <Field type=\"text\" name=\"email\" />\n              <ErrorMessage name=\"email\" component=\"div\" className='data-error'/>\n              <div className='password'>\n                <div>\n                  <div className='label'>* Password</div>\n                  <Field type=\"password\" name=\"password\" />\n                  <ErrorMessage name=\"password\" component=\"div\" className='data-error'/>\n                </div>\n                <div>\n                  <div className='label'>* Confirm Password</div>\n                  <Field type=\"password\" name=\"confirmPassword\" />\n                  <ErrorMessage name=\"confirmPassword\" component=\"div\" className='data-error'/>\n                </div>\n              </div>\n              <div className='check-wrapper'>\n                <Field type=\"checkbox\" name=\"acceptTerms\" className='check-input'/>\n                <label htmlFor=\"acceptTerms\" className=\"check\">I agree all the Term, Privacy Policy</label>\n              </div>\n              <ErrorMessage name=\"acceptTerms\" component=\"div\" className='data-error'/>\n              {loading ? \n                <div className='btn'>\n                  <div className='loading'></div>\n                </div>\n              : failed ? <button className='btn' type='submit'>Repeat</button> :\n              <button className='btn' type='submit'>SIGN UP</button> \n              }\n            </Form>\n          )}\n        </Formik> \n        </>\n      }\n      </div>\n      <div className='signUp-right'>\n       <div className='content'>\n        <h2 className='title'>Already have an Account?</h2>\n        <p className='text'>Sign in to access your account</p>\n        <button className='btn' onClick={() => setSignUp(false)}>SIGN IN</button>\n       </div>\n      </div>\n    </section>\n  )\n}\n\nexport default SignUp\n","/Users/user/Desktop/myprojects/DropMakers/src/components/Profile/Navigation.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/actions/googleAuth.js",["209","210"],"import {auth} from '../../axios/axios';\n\nexport const handleGoogleActionCreator = () => dispatch => {\n  window.gapi.load('auth2', function() {\n    window.gapi.auth2.init({\n      // client_id: process.env.REACT_APP_GOOGLE_ID_CLIENT\n      client_id: '1014024975064-8chcqru11epknmc560k3eooth12b1hp9.apps.googleusercontent.com'\n    })\n    .then((res) => {\n      console.log('success', res)\n    })\n    .catch((e) => {\n      console.log('error', e)\n    })\n  })\n}\n\nexport const handleGoogleSignInActionCreator = () => dispatch => {\n  const GoogleAuth = window.gapi.auth2.getAuthInstance()\n    GoogleAuth.signIn()\n    .then(googleUser => {\n      const profile = googleUser.getBasicProfile()\n      const id_token = googleUser.getAuthResponse().id_token;\n      console.log('code=' + id_token)\n      // console.log('Full Name: ' + profile.getName());\n      // console.log('Given Name: ' + profile.getGivenName());\n      // console.log('Family Name: ' + profile.getFamilyName());\n      // console.log(\"Image URL: \" + profile.getImageUrl());\n      // console.log(\"Email: \" + profile.getEmail());\n    })\n    .catch((e) => {\n      console.log('error', e)\n    })\n}\n\nexport const handleGoogleSignOutActionCreator = () => dispatch => {\n  const GoogleAuth = window.gapi.auth2.getAuthInstance()\n  GoogleAuth.signOut().then(() => {\n    console.log('signOut')\n  })\n}\n","/Users/user/Desktop/myprojects/DropMakers/src/store/actions/signIn.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/actions/themeSwitcher.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/store/actions/signUp.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Dashboards/Circle.js",[],"/Users/user/Desktop/myprojects/DropMakers/src/components/Dashboards/Notifications.js",[],{"ruleId":"211","replacedBy":"212"},{"ruleId":"213","replacedBy":"214"},{"ruleId":"215","severity":1,"message":"216","line":26,"column":6,"nodeType":"217","endLine":26,"endColumn":13,"suggestions":"218"},{"ruleId":"219","severity":1,"message":"220","line":6,"column":17,"nodeType":"221","messageId":"222","endLine":6,"endColumn":24},{"ruleId":"215","severity":1,"message":"223","line":27,"column":6,"nodeType":"217","endLine":27,"endColumn":8,"suggestions":"224"},{"ruleId":"219","severity":1,"message":"225","line":1,"colum